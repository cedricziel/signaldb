{
  "name": "signaldb-datasource",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:backend": "./build-backend.sh",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "dev:backend": "cargo watch -x 'build --manifest-path backend/Cargo.toml' -s 'npm run copy:backend'",
    "copy:backend": "./build-backend.sh",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Signaldb",
  "license": "Apache-2.0",
  "devDependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^2.2.0",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.7",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^62.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^10.5.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.97.3",
    "sass-loader": "16.0.6",
    "semver": "^7.6.3",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@grafana/data": "^12.2.0",
    "@grafana/i18n": "^12.2.0",
    "@grafana/runtime": "^12.2.0",
    "@grafana/schema": "^12.2.0",
    "@grafana/ui": "^12.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "packageManager": "pnpm@9.15.4"
}
